<odoo>
    <record id="pruebamedica_view" model="ir.ui.view">
        <field name="name">prueba_medica.calendar</field>
        <field name="model">veterinariaupo.pruebamedica</field>
        <field name="arch" type="xml">
            <calendar string="Pruebas Médicas" date_start="fecha" date_stop="fecha" color="fecha">
                <field name="descripcion"/>
                <field name="tipo"/>
                <field name="fecha"/>
            </calendar>
        </field>
    </record>

    <record id="prueba_medica_calendar_action" model="ir.actions.act_window">
        <field name="name">Pruebas Médicas</field>
        <field name="res_model">veterinariaupo.pruebamedica</field>
        <field name="view_mode">calendar,tree,form</field>
    </record>
</odoo>
